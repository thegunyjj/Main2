<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- outer circle -->
    <item>
        <shape android:shape="rectangle" >
            <corners android:radius="2dp" />
            <solid android:color="#01000000" />
        </shape>
    </item>

   <item
       android:bottom="1dp"
       android:left="1dp"
       android:right="1dp"
       android:top="1dp">
       <shape android:shape="rectangle">
           <corners android:radius="2dp" />
           <solid android:color="#03000000" />
       </shape>
   </item>

   <item
       android:bottom="2dp"
       android:left="2dp"
       android:right="2dp"
       android:top="2dp">
       <shape android:shape="rectangle">
           <corners android:radius="2dp" />
           <solid android:color="#05000000" />
       </shape>
   </item>

    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape android:shape="rectangle">
            <corners android:radius="2dp" />
            <solid android:color="#09000000" />
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape android:shape="rectangle">
            <corners android:radius="2dp" />
            <solid android:color="#14000000" />
        </shape>
    </item>
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <shape android:shape="rectangle">
            <corners android:radius="2dp" />
            <solid android:color="#fff" />
        </shape>
    </item>
</layer-list>